{
  "name": "@andrewda/prerender-spa-plugin",
  "version": "1.0.4",
  "description": "Flexible, framework-agnostic static site generation for sites and SPAs built with webpack.",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "babel es6 --out-dir es5-autogenerated",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "es6/*",
    "es5-autogenerated/*"
  ],
  "keywords": [
    "prerender",
    "spa",
    "ssr",
    "serverside-rendering"
  ],
  "author": "Andrew Dassonville <dassonville.andrew@gmail.com>",
  "contributors": [
    "Chris Fritz <chrisvfritz@gmail.com> (https://twitter.com/chrisvfritz)",
    "Drew Lustro (https://drewlustro.com/)",
    "Joshua Bemenderfer <tribex10@gmail.com> (https://joshderf.com/)",
    "qkdreyer <quentin.dreyer@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewda/prerender-spa-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/andrewda/prerender-spa-plugin/issues",
    "email": "dassonville.andrew@gmail.com"
  },
  "dependencies": {
    "@prerenderer/prerenderer": "^1.0.3",
    "@prerenderer/renderer-puppeteer": "^1.0.4",
    "html-minifier": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/eslint-parser": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "ava": "^3.15.0",
    "eslint": "^7.19.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "homepage": "https://github.com/andrewda/prerender-spa-plugin#readme",
  "directories": {
    "example": "examples",
    "test": "test"
  }
}
